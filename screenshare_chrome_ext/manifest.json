{
  "name": "Share Screen",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "Share control of any web page with friends.",

  "web_accessible_resources": [ "resources/*" ],

  "content_scripts": [
    {
    "all_frames": false,
    "matches": ["http://floating-ocean-5528.herokuapp.com/*", "file://*"],
    "exclude_matches": [],
    "js": [
      "jquery.js", "contentscript.js"
    ]
    }
  ]

}


